umn":33},"generated":{"line":1,"column":1961}},{"source":"main.ts","name":null,"original":{"line":98,"column":4},"generated":{"line":1,"column":1968}},{"source":"main.ts","name":null,"original":{"line":98,"column":11},"generated":{"line":1,"column":1976}},{"source":"main.ts","name":null,"original":{"line":99,"column":15},"generated":{"line":1,"column":1987}},{"source":"main.ts","name":null,"original":{"line":99,"column":15},"generated":{"line":1,"column":1994}},{"source":"main.ts","name":null,"original":{"line":99,"column":20},"generated":{"line":1,"column":1999}}],"sources":{"main.ts":"import axios from \"axios\";\nimport * as M from \"materialize-css\";\nimport Vue from \"vue\";\n\n// tslint:disable-next-line no-unused-expression\nconst app = new Vue( {\n    computed: {\n        hazPosts(): boolean {\n            return this.isLoading === false && this.posts.length > 0;\n        },\n        noPosts(): boolean {\n            return this.isLoading === false && this.posts.length === 0;\n        }\n    },\n    data() {\n        return {\n            content: \"\",\n            isLoading: true,\n            postEditingId: \"\",\n            posts: [],\n            selectedPost: \"\",\n            selectedPostId: 0,\n            title: \"\",\n            updated_at: \"\",\n        };\n    },\n    el: \"#app\",\n    methods: {\n        addPost() {\n            const post = {\n                content: this.content,\n                title: this.title\n            };\n            axios\n                .post( \"/api/posts\", post )\n                .then( () => {\n                    this.$refs.title.focus();\n                    this.content = \"\";\n                    this.loadPosts();\n                } )\n                .catch( ( err: any ) => {\n                    // tslint:disable-next-line:no-console\n                    console.log( err );\n                } );\n        },\n        confirmDeletePost( id: string ) {\n            const post = this.posts.find( ( p ) => p.id === id );\n            this.selectedPost = `${ post.title } ${ post.content }`;\n            this.selectedPostId = post.id;\n            const dc = this.$refs.deleteConfirm;\n            const modal = M.Modal.init( dc );\n            modal.open();\n        },\n        deletePost( id: string ) {\n            axios\n                .delete( `/api/posts/${ id }` )\n                .then( this.loadPosts )\n                .catch( ( err: any ) => {\n                    // tslint:disable-next-line:no-console\n                    console.log( err );\n                } );\n        },\n        loadPosts() {\n            axios\n                .get( \"/api/posts\" )\n                .then( ( res: any ) => {\n                    this.isLoading = false;\n                    this.posts = res.data;\n                } )\n                .catch( ( err: any ) => {\n                    // tslint:disable-next-line:no-console\n                    console.log( err );\n                } );\n        },\n        setToEditing( id: string) {\n          this.postEditingId = id;\n\n          setTimeout(() => {\n            document.getElementById(`title-edit-${id}`).focus();\n          }, 1);\n        },\n        editPost( id: string ) {\n            this.postEditingId = \"\";\n            const post = {\n                content: this.content,\n                id,\n                title: this.title\n            };\n            axios\n                .patch( `/api/posts/${ id }`, post )\n                .then( this.loadPosts() )\n                .catch( ( err: any ) => {\n                    // tslint:disable-next-line:no-console\n                    console.log( err );\n                } );\n        }\n    },\n    mounted() {\n        return this.loadPosts();\n    }\n} );\n"},"lineCount":null}},"error":null,"hash":"a260557c02a16e90b223c3bd604f9fb3","cacheData":{"env":{}}}